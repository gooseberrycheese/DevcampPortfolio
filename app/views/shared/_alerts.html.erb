<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: "Kaden Ficklin Portfolio", sticky: false, time: 2000) %>
  <% elsif flash[:error] %>
    <%= js add_gritter(flash[:error], title: "Kaden Ficklin Portfolio", sticky: false, time: 2000) %>  
  <% else %>
    <%= js add_gritter(flash[:notice], title: "Kaden Ficklin Portfolio", sticky: false, time: 2000) %>
<% end %>